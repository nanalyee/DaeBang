<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.ssafy.vue.model.mapper.SubwayMapper">
	<select id="listSubwayInfo" parameterType="map" resultType="SubwayDto">
		select *
		from subway_info
		order by stationCode asc
	</select>
</mapper>

